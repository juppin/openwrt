/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9341.dtsi"

/ {
	model = "TP-Link WR841 v8";
	compatible = "tplink,tl-wr841-v8";

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x2000000>;
	};

	leds {
		compatible = "gpio-leds";

		wlan {
			label = "tp-link:green:wlan";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		qss {
			label = "tp-link:green:qss";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		wan {
			label = "tp-link:green:wan";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan1 {
			label = "tp-link:green:lan1";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan2 {
			label = "tp-link:green:lan2";
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan3 {
			label = "tp-link:green:lan3";
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		lan4 {
			label = "tp-link:green:lan4";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};

		system {
			label = "tp-link:green:system";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			default-state = "off";
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		button@0 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		button@1 {
			label = "wifi";
			linux,code = <KEY_RFKILL>;
			gpios = <&gpio 16 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};
	};
};

&ref {
	clock-frequency = <25000000>;
};

&uart {
	status = "okay";
};

&gpio {
	status = "okay";
};

&spi {
	num-chipselects = <1>;
	num-cs = <1>;

	status = "okay";

	flash@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot:	partition@0 {
				label = "u-boot";
				reg = <0x000000 0x020000>;
				read-only;
			};

			partition@20000 {
				label = "firmware";
				reg = <0x020000 0x3d0000>;
			};

			partition@3f0000 {
				label = "art";
				reg = <0x3f0000 0x010000>;
				read-only;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	//regmap = <&eth1>;
	phy-mask = <0x0>;

	phy4: ethernet-phy@4 {
		reg = <4>;
		phy-mode = "mii";
	};
};

&eth0 {
	status = "okay";

	mtd-mac-address = <&uboot 0x1fc00>;
	mtd-mac-address-increment = <1>;

	phy-mode = "mii";
	phy-handle = <&phy4>;

	fixed-link {
		speed = <100>;
		full-duplex;
	};
};

&mdio1 {
        status = "okay";

        phy0: ethernet-phy@0 {
                reg = <0>;
                phy-mode = "gmii";
        };
};

&eth1 {
        status = "okay";

	mtd-mac-address = <&uboot 0x1fc00>;

	phy-mode = "gmii";
	phy-handle = <&phy0>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};

	gmac-config {
		device = <&gmac>;

		/* switch-phy-addr-swap = <1>; */
		switch-phy-swap = <1>;
	};
};
